{
  "buildCommand": "cd zzik-live && npm run build",
  "devCommand": "cd zzik-live && npm run dev",
  "installCommand": "cd zzik-live && npm install",
  "framework": "nextjs",
  "outputDirectory": "zzik-live/.next",
  "regions": ["icn1"],
  "env": {
    "NEXT_PUBLIC_APP_URL": "@app-url"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
